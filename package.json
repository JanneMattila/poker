{
  "name": "texas-holdem-poker",
  "version": "1.0.0",
  "description": "Texas Hold'em Poker game with offline and online multiplayer support",
  "type": "module",
  "main": "src/server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "server:dev": "nodemon src/server/server.js",
    "server:prod": "node src/server/server.js",
    "client:dev": "vite",
    "console": "node src/console-client/client.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "poker",
    "texas-holdem",
    "multiplayer",
    "websocket",
    "node.js"
  ],
  "author": "JanneMattila",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "vite": "^4.4.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
